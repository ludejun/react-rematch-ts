{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        // "target": {},
        "modules": false
      }
    ],
    "@babel/react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    ["@babel/plugin-transform-runtime", { "corejs": 3 }]
  ]
}
