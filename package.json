    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "webpack": "^5.58.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
